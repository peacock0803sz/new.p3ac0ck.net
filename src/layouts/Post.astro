---
import "../styles/markdown.css";
import Base from "./Base.astro";
const { frontmatter } = Astro.props;
---

<Base title={frontmatter.title} head="Peacock's Blog">
  <h1 class="text-5xl font-bold pb-10">{frontmatter.title}</h1>
  <p class="text-right">公開日: {frontmatter.published}</p>
  <article class="markdown-body">
    <slot />
  </article>
</Base>
